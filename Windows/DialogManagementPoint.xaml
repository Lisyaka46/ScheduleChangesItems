<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScheduleChangesItems.Windows"
        xmlns:GUI="clr-namespace:ScheduleChangesItems.GUI" x:Class="ScheduleChangesItems.Windows.DialogManagementPoint"
        mc:Ignorable="d"
        Title="Добавление позиции тенденции" Height="158" Width="466" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="85*"/>
            <ColumnDefinition Width="89*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="36"/>
            <RowDefinition Height="33*"/>
            <RowDefinition Height="73*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="TextName" TextWrapping="Wrap" VerticalAlignment="Center" Margin="79,0,6,0" MinWidth="89" BorderThickness="0,0,0,2" Height="18" SelectionBrush="#FF13EA0C" FontWeight="Bold"/>
        <TextBlock TextWrapping="Wrap" Text="Имя точки:" VerticalAlignment="Center" Margin="3,0,128,0" FontSize="14" Height="18"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Значение*:" VerticalAlignment="Center" Grid.Column="1" Margin="1,0,0,0" FontSize="14" Height="18" Width="70"/>
        <TextBox x:Name="TextValue" Grid.Column="1" TextWrapping="Wrap" Text="80" VerticalAlignment="Center" Margin="77,0,7,0" MinWidth="90" FontWeight="Bold" BorderThickness="0,0,0,2" Height="18" SelectionBrush="#FF13EA0C"/>
        <GUI:TextButton x:Name="ButtonComplete" Grid.Column="1" Grid.Row="2" Texting="Создать" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFD4D395" MillisecondsAnimation="80" MouseBackground="#FFEDECBD" CornerRadius="4,4,4,4" TextMargin="6,6,6,6" FontFamily="Arial" FontSizeText="15" Margin="8,15,5,15"/>
        <GUI:TextButton x:Name="ButtonValue_pl1" Grid.Column="1" Grid.Row="1" Texting="+1" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFD4D395" MillisecondsAnimation="80" MouseBackground="#FFEDECBD" CornerRadius="4,4,4,4" TextMargin="4,4,4,4" FontFamily="Arial" FontSizeText="12" Margin="8,2,0,2" Width="24" HorizontalAlignment="Left"/>
        <GUI:TextButton x:Name="ButtonValue_pl5" Grid.Column="1" Grid.Row="1" Texting="+5" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFD4D395" MillisecondsAnimation="80" MouseBackground="#FFEDECBD" CornerRadius="4,4,4,4" TextMargin="2,2,2,2" FontFamily="Arial" FontSizeText="12" Margin="37,2,0,2" Width="27" HorizontalAlignment="Left"/>
        <GUI:TextButton x:Name="ButtonValue_pl10" Grid.Column="1" Grid.Row="1" Texting="+10" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFD4D395" MillisecondsAnimation="80" MouseBackground="#FFEDECBD" CornerRadius="4,4,4,4" TextMargin="2,2,2,2" FontFamily="Arial" FontSizeText="12" Margin="69,2,0,2" Width="27" HorizontalAlignment="Left"/>
