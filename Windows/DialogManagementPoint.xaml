        <GUI:TextButton x:Name="ButtonValue_min1" Grid.Row="1" Texting="-1" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFA9A998" MillisecondsAnimation="80" MouseBackground="#FFCECEC7" CornerRadius="4,4,4,4" TextMargin="4,4,4,4" FontFamily="Arial" FontSizeText="12" Margin="195,2,0,2" Width="24" HorizontalAlignment="Left"/>
