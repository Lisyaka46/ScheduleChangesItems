        <GUI:TextButton x:Name="ButtonValue_pl30" Grid.Column="1" Grid.Row="1" Texting="+30" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFD4D395" MillisecondsAnimation="80" MouseBackground="#FFEDECBD" CornerRadius="4,4,4,4" TextMargin="2,2,2,2" FontFamily="Arial" FontSizeText="12" Margin="134,2,0,2" Width="27" HorizontalAlignment="Left"/>
