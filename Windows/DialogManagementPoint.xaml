        <GUI:TextButton x:Name="ButtonValue_min15" Grid.Row="1" Texting="-15" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFA9A998" MillisecondsAnimation="80" MouseBackground="#FFCECEC7" CornerRadius="4,4,4,4" TextMargin="4,4,4,4" FontFamily="Arial" FontSizeText="12" Margin="105,2,0,2" Width="27" HorizontalAlignment="Left"/>
        <GUI:TextButton x:Name="ButtonValue_min30" Grid.Row="1" Texting="-30" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFA9A998" MillisecondsAnimation="80" MouseBackground="#FFCECEC7" CornerRadius="4,4,4,4" TextMargin="4,4,4,4" FontFamily="Arial" FontSizeText="12" Margin="74,2,0,2" Width="27" HorizontalAlignment="Left"/>
        <GUI:TextButton x:Name="ButtonValue_Com" Grid.Row="1" Texting="+0" ButtonBackground="White" DisabledBackground="#FFA97B7B" ClickedBackground="#FFA9A998" MillisecondsAnimation="80" MouseBackground="#FFCECEC7" CornerRadius="4,4,4,4" TextMargin="4,4,4,4" FontFamily="Cambria" FontSizeText="12" Margin="6,2,0,2" Width="64" HorizontalAlignment="Left" FontWeight="Bold" FontStyle="Italic"/>
