<Window x:Class="ScheduleChangesItems.Windows.WindowSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScheduleChangesItems.Windows"
        mc:Ignorable="d"
        Title="WindowSettings" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="633*"/>
        </Grid.ColumnDefinitions>
        <Grid>
            <TreeView Margin="3,3,3,3" BorderThickness="0,0,0,0" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">
                <Button x:Name="GeneralSettings" Content="Общие"/>
            </TreeView>
            <Rectangle Stroke="Black" RadiusX="8" StrokeThickness="3" RadiusY="8"/>
        </Grid>
        <Frame x:Name="FrameSettings" Grid.Column="1"/>

    </Grid>
</Window>
