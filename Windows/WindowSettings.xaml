<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScheduleChangesItems.Windows"
        xmlns:GUI="clr-namespace:ScheduleChangesItems.GUI" x:Class="ScheduleChangesItems.Windows.WindowSettings"
        mc:Ignorable="d"
        Title="WindowSettings" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="633*"/>
        </Grid.ColumnDefinitions>
        <Grid>
            <GUI:TextButton x:Name="ButtonGeneralSettings" ButtonBackground="White" CornerRadius="9,9,9,9" MillisecondsAnimation="80" TextMargin="5,3,5,3" Texting="Общие" DisabledBackground="#FFA97B7B" MouseBackground="#FF6FCAD6" ClickedBackground="#FF5CA2AB" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="4,7,0,0" Panel.ZIndex="1"/>
            <Rectangle Stroke="Black" StrokeThickness="2" RadiusX="1" RadiusY="8" Margin="-3,0,0,0"/>
        </Grid>
        <Frame x:Name="FrameSettings" Grid.Column="1" Focusable="False"/>

    </Grid>
</Window>
